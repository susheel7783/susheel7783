#include<bits/stdc++.h>
using namespace std;
int main(){
    int i=65;
    char c=i;     //typecasting 
    int *p=&i;
    char *pc=(char *)p;
    cout<<p<<endl;
    cout<<*pc<<endl;
    cout<<*p<<endl;
    cout<<pc<<endl;
    cout<<*(pc+1)<<endl;  // actually this give null means blank space
     cout<<*(pc+2)<<endl;
      cout<<*(pc+3)<<endl;
}

