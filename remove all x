#include <bits/stdc++.h>
using namespace std;
string removeAllx(string s,char ch){
   int n=s.length();
   string str="";
   for(int i=0;i<n;i++){
       if(s[i]!=ch){
           str=str+s[i];
       }
   }
   return str;
}
int main()
{
        char ch='g';
    string s="gxxksfxrgxxks";
cout<<removeAllx(s,ch);
    return 0;
}

