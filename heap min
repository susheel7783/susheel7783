// heap.cpp
#include<iostream>
#include<queue>
using namespace std;
int main(){
	priority_queue<int,vector<int>,greater<int>>h; //for max heap it give res in decreasing order
	h.push(1); //  min heap it gives res in decreasing order
	h.push(13);
	h.push(2);
	h.push(5);
	while(!h.empty()){
		cout<<h.top()<<" ";
		h.pop();
	}
	
}

// 1 2 5 13
