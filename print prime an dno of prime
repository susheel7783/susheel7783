#include<bits/stdc++.h>
using namespace std;
bool isPrime(int n){
    for(int i=2;i<n;i++){
        if(n%i==0){
            return false;
        }
    }
    return true;
}
void print(int N){

    for(int i=2;i<=N;i++){
        if(isPrime(i)){
           
            cout<<i<<" ";
             
        }
    }
}
int count(int n){
    int cnt=0;
    for(int i=2;i<=n;i++){
        if(isPrime(i)){
            cnt++;
        }
        
    }
    return cnt;
}
int main(){
        int n;
        cin>>n;
        
  print(n);
  cout<<"no of prime no:"<<count(n);
}
