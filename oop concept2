#include<iostream>
using namespace std;

class car{
    public:
    string name;
    int price;
    int mileage;
    car(){ //default contructor
        cout<<"default contructor"<<endl;
    }
    car(string n,int p,int m){  //parametrized contructor
        cout<<"parametrized contructor"<<endl;
        name=n;
        price=p;
        mileage=m;
    }
    void print(){
        cout<<"Name: "<<name<<endl;
        cout<<"Price :"<<price<<endl;
        cout<<"mileage :"<<mileage<<endl;
    }
};
int main(){
